{
	// Don't check in sensitive data!
	// Look at https://docs.microsoft.com/en-us/aspnet/core/security/app-secrets eventually.
	"DBLocation": "/database/Notifications.db",
	"NotificationSID": "SID",
	"ClientSecret": "secret",
	"WinchattyAPIBase": "https://winchatty.com/v2/",
	"HostUrl": "http://0.0.0.0:4000",

	"Serilog": {
		"Using": [ "Serilog.Sinks.RollingFile" ],
		"MinimumLevel": "Verbose",
		"WriteTo": [
			{ "Name": "ColoredConsole" },
			{
				"Name": "RollingFile",
				"Args": {
					"pathFormat": "/log/notifications-{Date}.log"
				}
			}
		]
	}

}